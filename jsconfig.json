{
  "compilerOptions": {
    "target": "es5",
    "module": "esnext",
    "baseUrl": "./",
    "moduleResolution": "node",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  },
   // 禁用template的报错提示,和export default 的波浪线报错提示
   "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true,
    "experimentalShamefullySupportOptionsApi": false
  },
}
